{
  "name": "2025-annual",
  "version": "1.0.0",
  "description": "Testing Best Practices Dashboard",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "tutorial": "node scripts/open-tutorial.js",
    "tutorial:setup-exercise-5": "node scripts/tutorial-setup-exercise-5.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test --ui",
    "test:e2e:headless": "npx playwright test",
    "demo:break": "node scripts/demo-break.js",
    "demo:fix": "node scripts/demo-fix.js",
    "demo:validate": "node scripts/demo-break.js && node scripts/demo-fix.js",
    "demo:setup-broken": "node scripts/demo-setup-broken.js",
    "demo:setup-no-filter": "node scripts/demo-setup-no-filter.js",
    "demo:reset": "node scripts/demo-reset.js",
    "demo:bug-01": "node scripts/demo-bug-01.js",
    "demo:bug-02": "node scripts/demo-bug-02.js",
    "demo:bug-03": "node scripts/demo-bug-03.js",
    "demo:bug-04": "node scripts/demo-bug-04.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ag-grid-community": "^35.0.1",
    "express": "^5.2.1",
    "open": "^11.0.0",
    "sortablejs": "^1.15.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^25.0.1",
